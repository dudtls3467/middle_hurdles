<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ordersProduct">
	<insert id="ordersProductInsert" parameterType="ordersProductVo">
		INSERT INTO ORDERS_PRODUCT
		VALUES (ORDERS_PRODUCT_SEQ.NEXTVAL, #{ordersNo}, #{productNo}, #{ordersProductQty})
	</insert>

</mapper>